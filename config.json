{
  "targetProject": "./examples/example-project",
  "outputDir": "./output",
  "fileExtensions": [".vue", ".js", ".ts"],
  "excludeDirs": [
    "node_modules",
    "dist",
    ".git",
    "build",
    "public"
  ],
  "excludeFiles": [
    "*.min.js",
    "*.test.js",
    "*.spec.js"
  ],
  "encoding": "utf-8",
  "autoReplace": {
    "enabled": true,
    "backup": true,
    "backupDir": "./backup",
    "preview": false,
    "i18nPath": "./src/i18n",
    "importPath": "@/i18n",
    "generateEnglish": false,
    "keyStrategy": "semantic",
    "commonKeys": [
      "common.submit",
      "common.cancel",
      "common.confirm",
      "common.delete",
      "common.edit",
      "common.save"
    ]
  },
  "keyMappings": {
    "提交": "common.submit",
    "取消": "common.cancel",
    "确认": "common.confirm",
    "删除": "common.delete",
    "编辑": "common.edit",
    "保存": "common.save"
  },
  "keyPrefixes": {
    "src/views/user/": "user.",
    "src/views/admin/": "admin.",
    "src/components/common/": "common."
  }
}

{
  "targetProject": "./examples/vue2-demo",
  "outputDir": "./output",
  "logDir": "./logs",
  "logLevel": "info",
  "enableFileLog": true,
  "fileExtensions": [".vue", ".js", ".ts"],
  "excludeDirs": [
    "node_modules",
    "dist",
    ".git",
    "build",
    "public",
    "i18n",
    "locales",
    "lang"
  ],
  "excludeFiles": [
    "*.min.js",
    "*.test.js",
    "*.spec.js"
  ],
  "encoding": "utf-8",
  "autoReplace": {
    "enabled": true,
    "backup": true,
    "backupDir": "./backup",
    "preview": false,
    "importPath": "@/lang",
    "generateEnglish": false,
    "keyStrategy": "semantic",
    "commonKeys": [
      "Common.submit",
      "Common.cancel",
      "Common.confirm",
      "Common.delete",
      "Common.edit",
      "Common.save"
    ]
  },
  "keyMappings": {
    "提交": "Common.submit",
    "取消": "Common.cancel",
    "确认": "Common.confirm",
    "删除": "Common.delete",
    "编辑": "Common.edit",
    "保存": "Common.save"
  },
  "keyPrefixes": {
    "src/views/user/": "User.",
    "src/views/admin/": "Admin.",
    "src/components/common/": "Common."
  },
  "pendingTasks": {
    "enabled": true,
    "outputFormat": ["markdown", "json"],
    "includeSkipped": true,
    "includeFailed": true
  }
}
